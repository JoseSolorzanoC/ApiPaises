<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://city.interfaces.generated.soapws.apipaises.appdist.uteq" targetNamespace="http://city.interfaces.generated.soapws.apipaises.appdist.uteq" elementFormDefault="qualified">
    <xs:complexType name="city">
        <xs:sequence>
            <xs:element name="cityId" type="xs:int" nillable="true" />
            <xs:element name="cityName" >
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="70"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cityFlag" >
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="150"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cityCabecera" >
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="70"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cityAlt" type="xs:float" nillable="true" />
            <xs:element name="cityLat" type="xs:float" nillable="true" />
            <xs:element name="cityCallCode" >
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="5"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cityState" >
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cityProvinceId" type="xs:int" nillable="true" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="serviceStatus">
        <xs:sequence>
            <xs:element name="status" type="xs:string" />
            <xs:element name="message" type="xs:string" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="cities">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="city" type="tns:city" />
        </xs:sequence>
    </xs:complexType>

    <xs:element name="getCityByIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="cityId" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getCityByIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" name="city" type="tns:city" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getCitiesByProvinceIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="provinceId" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getCitiesByProvinceIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="cities" type="tns:cities" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="addCityRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="city" type="tns:city" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="addCityResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus" />
                <xs:element name="city" type="tns:city" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="updateCityRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="city" type="tns:city" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="updateCityResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus" />
                <xs:element name="city" type="tns:city" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="deleteCityRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="cityId" type="xs:int" />
                <xs:element name="cityState" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="deleteCityResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>